{
    "name": "etchfoda/recaptcha",
    "description": "Invisible reCAPTCHA For Laravel.",
    "keywords": ["recaptcha", "no-captcha", "captcha", "invisible", "invisible-recaptcha", "laravel", "laravel5", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "EtchFoda",
            "homepage": "https://www.etchfoda.com"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "illuminate/view": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "EtchFoda\\InvisibleReCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EtchFoda\\InvisibleReCaptcha\\InvisibleReCaptchaServiceProvider"
            ]
        }
    }
}
